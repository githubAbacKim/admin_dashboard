 <script>
  var a;
  function myFunction(){
      if(a==1)
          {
              document.getElementById("Mydiv").style.display="none";
              return a=0;
          }
      else
          {
              document.getElementById("Mydiv").style.display="inline";
              return a=1;
          }
  }
  </script>
